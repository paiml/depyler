{
  "name": "depyler-playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "bench": "vitest bench",
    "wasm:build": "cd ../crates/depyler-wasm && wasm-pack build --target web --out-dir ../../playground/public/wasm",
    "wasm:optimize": "wasm-opt -Oz --enable-bulk-memory -o public/wasm/optimized.wasm public/wasm/depyler_playground_bg.wasm"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@types/d3": "^7.4.3",
    "d3": "^7.8.5",
    "lru-cache": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.8.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest-axe": "^8.0.0",
    "jsdom": "^24.0.0",
    "playwright": "^1.42.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.9",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^3.2.4",
    "wasm-opt": "^1.3.0"
  }
}
